# ----------------------------------------------------------------------------
# Maven specific ignores
# ----------------------------------------------------------------------------
# Generated target directory
/target/
# Maven project files
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties
dependency-reduced-pom.xml
# Log files generated by Maven builds
build.log
# Maven wrappers
.mvn/wrapper/maven-wrapper.jar

# ----------------------------------------------------------------------------
# Java / JVM specific ignores
# ----------------------------------------------------------------------------
# Compiled class files
*.class
# JAR and WAR files
*.jar
*.war
# AOT related files
/src/main/generated/
/src/main/resources/application-aot.properties
/src/main/resources/META-INF/native-image/
# Log files
*.log
*.txt
# Backup files
*.bak
# JNI/native development
*.dll
*.so
*.dylib*
# Eclipse/IntelliJ/NetBeans configuration files (though VS Code is primary)
.project
.classpath
.settings/
.idea/
*.iws
*.iml
*.ipr

# ----------------------------------------------------------------------------
# Operating System temporary files
# ----------------------------------------------------------------------------
# Thumbnails
.DS_Store
# Windows files
Thumbs.db
Desktop.ini

# ----------------------------------------------------------------------------
# Spring Boot specific ignores
# ----------------------------------------------------------------------------
# Embedded Tomcat logs, if created
/logs/
# Application dump file (if configured)
dump.rdb
# Spring Boot CLI temporary files
.spring-boot-cli/

# ----------------------------------------------------------------------------
# Visual Studio Code specific ignores
# ----------------------------------------------------------------------------
# VS Code directory for workspace-specific settings, tasks, and debug configurations
.vscode/
# Workspace files (can sometimes be auto-generated by VS Code)
*.code-workspace
# History and cache files generated by VS Code extensions or language servers
# Examples: 
.lsp-cache/
.java/
.settings/

# ----------------------------------------------------------------------------
# Test Coverage reports
# ----------------------------------------------------------------------------
/jacoco-output/
